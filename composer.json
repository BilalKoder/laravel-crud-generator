{
    "name": "bilalkoder/laravel-crud-generator",
    "description": "Laravel CRUD Generator â€” Build APIs in Seconds!",
    "keywords": [
        "laravel",
        "crud",
        "api",
        "generator",
        "bilalkoder",
        "laravel-crud-generator"
    ],
    "homepage": "https://github.com/laravelcrudgenerator/laravel-crud-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Syed Bilal Ali",
            "email": "bilalaly1122@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
      "php": "^8.0",
      "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravelcrudgenerator\\LaravelCrudGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelcrudgenerator\\LaravelCrudGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelcrudgenerator\\LaravelCrudGenerator\\LaravelCrudGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelCrudGenerator": "Laravelcrudgenerator\\LaravelCrudGenerator\\LaravelCrudGeneratorFacade"
            }
        }
    }
}
